# Test Environment Variables
# Copy this file to .env.test and fill in your test values

# Database
DATABASE_URL="postgresql://test:test@localhost:5432/prompteasy_test"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="test-secret-key-change-in-production"

# Public App URL (for share links)
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# AI Services (use test keys or leave empty to skip AI tests)
OPENAI_API_KEY="sk-test-key-or-leave-empty"
ANTHROPIC_API_KEY="test-key-or-leave-empty"

# OAuth (optional for E2E tests)
GOOGLE_CLIENT_ID="test-client-id"
GOOGLE_CLIENT_SECRET="test-client-secret"

# Okta (optional for enterprise tests)
OKTA_CLIENT_ID="test-okta-client-id"
OKTA_CLIENT_SECRET="test-okta-secret"
OKTA_ISSUER="https://test.okta.com"

# Redis (optional, for session tests)
REDIS_URL="redis://localhost:6379"

# Prompts.chat Integration
PROMPTS_CHAT_CSV_URL="https://raw.githubusercontent.com/f/awesome-chatgpt-prompts/main/prompts.csv"
